<UserControl x:Class="PapayagramsClient.WPFControls.WPFGamePiece"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PapayagramsClient.WPFControls"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="50">
    <Grid>
        <Rectangle x:Name="GamePiece" Fill="Green" Height="60" Width="50"/>
        <TextBlock Name="PieceLetter" Text="A" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25"/>
    </Grid>
</UserControl>
