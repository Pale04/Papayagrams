<Window x:Class="PapayagramsClient.PopUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PapayagramsClient" 
        xmlns:svg="clr-namespace:SVGImage.SVG;assembly=SVGImage"
        mc:Ignorable="d"
        Title="PopUpWindow" Height="200" Width="400">
    <Grid>
        <Label Name="MessageLabel"  HorizontalContentAlignment="Left" HorizontalAlignment="Left" VerticalAlignment="Top" MaxWidth="180" Margin="82,30,0,0" FontSize="20">
            <TextBlock TextWrapping="WrapWithOverflow"/>
        </Label>
        <svg:SVGImage Name="IconImage" Margin="20,29,323,100"/>
    </Grid>
</Window>
