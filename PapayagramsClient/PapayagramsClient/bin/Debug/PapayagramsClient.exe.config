<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IChatService">
                    <security>
                        <transport sslProtocols="None" />
                    </security>
                </binding>
                <binding name="NetTcpBinding_IGameService">
                    <security>
                        <transport sslProtocols="None" />
                    </security>
                </binding>
                <binding name="NetTcpBinding_ILoginService">
                    <security>
                        <transport sslProtocols="None" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8090/Contracts" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IChatService" contract="PapayagramsService.IChatService"
                name="NetTcpBinding_IChatService">
                <identity>
                    <userPrincipalName value="PALE\epale" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://localhost:8090/Contracts" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IGameService" contract="PapayagramsService.IGameService"
                name="NetTcpBinding_IGameService">
                <identity>
                    <userPrincipalName value="PALE\epale" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://localhost:8090/Contracts" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_ILoginService" contract="PapayagramsService.ILoginService"
                name="NetTcpBinding_ILoginService">
                <identity>
                    <userPrincipalName value="PALE\epale" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>